{
  "id": "material",
  "name": "Material",
  "version": "4.8.6",
  "vars": {
    "skin": {
      "title": "Skins",
      "value": "default",
      "options": [
        {
          "value": "default",
          "label": "Default",
          "primary": "#2681D5"
        },
        {
          "value": "deep_red",
          "label": "Deep Red",
          "primary": "#ac1a2f"
        },
        {
          "value": "deep_purple",
          "label": "Deep Purple",
          "primary": "#5a2d81"
        },
        {
          "value": "celtics",
          "label": "Celtics",
          "primary": "#2eff85"
        }
      ],
      "type": "skin",
      "id": "",
      "attr": ""
    },
    "brand-primary": {
      "title": "Brand Primary",
      "id": ".btn-primary",
      "attr": "background"
    },
    "brand-success": {
      "title": "Brand Success",
      "id": ".btn-success",
      "attr": "background"
    },
    "brand-info": {
      "title": "Brand Info",
      "id": ".btn-info",
      "attr": "background"
    },
    "brand-warning": {
      "title": "Brand Warning",
      "id": ".btn-warning",
      "attr": "background"
    },
    "brand-danger": {
      "title": "Brand Danger",
      "id": ".btn-danger",
      "attr": "background"
    },
    "header-bg": {
      "title": "Header Background",
      "id": ".sticky-bar",
      "attr": "background"
    },
    "nav-bg": {
      "title": "Navigation Background",
      "id": ".navbar.main-navigation",
      "attr": "background"
    },
    "nav-link-color": {
      "title": "Navigation Link Color",
      "id": ".navbar.main-navigation .site-menu > li > a",
      "attr": "color"
    },
    "nav-link-color-hover": {
      "title": "Navigation Link Color Hover, Active",
      "id": ".navbar.main-navigation .site-menu > li > a:hover, .navbar.main-navigation .site-menu > li > a.menu_is_selected",
      "attr": "color"
    },
    "body-bg": {
      "title": "Body Background",
      "id": "body",
      "attr": "background"
    },
    "text-color": {
      "title": "Body Text Color",
      "id": ".activity_feed_content .core-feed-title",
      "attr": "color"
    },
    "links-color": {
      "title": "Links",
      "id": "a",
      "attr": "color"
    },
    "links-color-hover": {
      "title": "Links Hover",
      "id": "a:hover",
      "attr": "color"
    },
    "content-bg": {
      "title": "Content Background ",
      "id": "#main #content-stage, .js_feed_view_more_entry_holder .feed-stream-content, .js_feed_comment_border .feed-stream-content, .header-filter-holder, div.comment_mini_content_border",
      "attr": "background"
    },
    "content-width": {
      "title": "Content Width",
      "id": ".container",
      "attr": "width"
    },
    "input-bg": {
      "title": "Form Input Background",
      "id": ".form-control, .js_box_holder .js_box .form-control, .js_box_holder .js_box input",
      "attr": "background"
    },
    "input-color": {
      "title": "Form Input Text Color",
      "id": ".form-control, .js_box_holder .js_box .form-control, .js_box_holder .js_box input",
      "attr": "color"
    },
    "block-bg": {
      "title": "Block Background",
      "id": "#main .layout-left .block, #main .layout-right .block",
      "attr": "background"
    },
    "block-title-color": {
      "title": "Block Title Text Color",
      "id": "#main .layout-left .block > .title, #main .layout-right .block > .title",
      "attr": "color"
    },
    "block-border-color": {
      "title": "Block Title Border Color",
      "id": "#main .layout-left .block > .content, #main .layout-right .block > .content",
      "attr": "border-color"
    },
    "footer-bg": {
      "title": "Footer Background",
      "id": "#section-footer",
      "attr": "background"
    },
    "footer-color": {
      "title": "Footer Text Color",
      "id": "#section-footer .footer-holder .copyright",
      "attr": "color"
    },
    "fixed-header": {
      "title": "Fix the Site Header",
      "type": "boolean",
      "attr": "mass_class",
      "mass_class": "fixed-preview"
    },
    "fixed-app-menu": {
      "title": "Fix the App Menu",
      "type": "boolean",
      "attr": "mass_class",
      "mass_class": "fixed-preview"
    },
    "fixed-profile-menu": {
      "title": "Fix the Profile Menu",
      "type": "boolean",
      "attr": "mass_class",
      "mass_class": "fixed-preview"
    },
    "fixed-left-columns": {
      "title": "Fix the left columns",
      "type": "boolean",
      "attr": "mass_class",
      "mass_class": "fixed-preview"
    },
    "fixed-right-columns": {
      "title": "Fix the right columns",
      "type": "boolean",
      "attr": "mass_class",
      "mass_class": "fixed-preview"
    }
  }
}
